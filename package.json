{
  "name": "cycle-autobahn",
  "version": "1.0.0",
  "description": "Cycle.js driver wrapper around autobahn",
  "main": "src/index.ts",
  "scripts": {
    "test": "nyc mocha --require tsconfig-paths/register --require ts-node/register test/**/*.ts",
    "test-watch": "mocha --require tsconfig-paths/register --require ts-node/register --watch --watch-extensions ts test/**/*.ts"
  },
  "keywords": [
    "cycle.js",
    "autobahn"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "all": true,
    "require": [
      "ts-node/register"
    ]
  },
  "author": "Michael Long",
  "license": "MIT",
  "devDependencies": {
    "@types/autobahn": "^18.10.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.2",
    "ws": "^7.0.1"
  },
  "dependencies": {
    "@cycle/run": "^5.2.0",
    "autobahn": "^19.4.1",
    "tsc": "^1.20150623.0",
    "xstream": "^11.10.0"
  }
}
